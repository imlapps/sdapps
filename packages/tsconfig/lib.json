{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "2.0.0",

  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "incremental": false,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "ES2022",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "ES2022"
  },
  "extends": ["./base.json"]
}
